{
  "name": "Zendesk Close All",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Insert button on ZD pages to close all ZD tabs.",
  "icons": {

  },
  "content_scripts":[{
      "matches": ["https://*.zendesk.com/*", "http://*.zendesk.com/*"],
      "run_at": "document_end",
      "all_frames": true,
      "js": ["contentScript.js"]
  }]
}
